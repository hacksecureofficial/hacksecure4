{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "target": "ES6",
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/courses/layout.js",
    "app/api/auth/[...nextauth]/route.ts"
, "app/api/signin/route.tsx", "app/api/signin/route.tsx", "app/api/test-attempts/route.js", "utils/mailer.js", "app/courses/payment/page.js", "app/courses/payment/page.js", "app/courses/payment/page.js", "app/courses/payment/page.js", "components/PromoSection.jsx", "components/FluidSimulation.jsx"  ],
  "exclude": ["node_modules"]
}
